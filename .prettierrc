{
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@components/(.*)$",
		"^@hooks/(.*)$",
		"^@shared/(.*)$",
		"^@services/(.*)$",
		"^@assets/(.*)$",
		"^@utils/(.*)$",
		"^@config/(.*)$",
		"^@store/(.*)$",
		"^@ui/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
